{% set PathComponents = '../../components/' %}
<!DOCTYPE html>
<html>
<head>
    <title>FrontendCore: The all in one frontend framework.</title>
    <meta charset="utf-8">
    <meta http-equiv="cache-control" content="max-age=0" />
    <meta http-equiv="cache-control" content="no-cache" />
    <meta http-equiv="expires" content="0" />
    <meta http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT" />
    <meta http-equiv="pragma" content="no-cache" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
    <meta name="description" content="The frontend framework for the real Life. A mix of frontend technologies that makes you 500% productive and be prepared for the future." />
    <meta name="robots" content="all" />
    <meta name="keywords" content="frontend, framework, sass, scss, javascript, requirejs, amd modules, scalable javascript architecture, grid sass" />
    <style>
        body { display: none; opacity: 0; }
    </style>
</head>
<body class="animated fade-in" data-fc-modules="polyfiller">
{% block fullmain %}
    <div class="layout">
        <aside id="menu" class="__side layout-side">
            <header class="ta-c">
                <a title="Frontend Core" href="{{ relative_path ~ urlbase }}index.html"><img src="{{ relative_path ~ url.static }}img/logo-frontendcore.png" width="200" class="va-m" alt="FRONTEND CORE" /></a>
            </header>
            {% block navigation %}
                <nav class="navigation _primary _vertical">
                    <ul>
                        {% for section in sections %}
                            <li>
                                <a href="{{ section.href }}" data-fc-modules="toggle">
                                    {{ section.name }}
                                    {% if section.children|length > 0 %}
                                        <i class="icon-angle-down fl-r"></i>
                                    {% endif %}
                                </a>
                                {% if section.children|length > 0 %}
                                    <div id="{{ section.id }}" {% if section.name != current_section %} class="hidden"{% endif %}>
                                        <ul>
                                            {% for subsection in section.children %}
                                                <li {% if subsection.name == current_children %} class="active"{% endif %}>
                                                    <a href="{{ relative_path ~ url.base  ~ section.id ~ '/'~  subsection.href }}" class="d-b"><i class="icon-angle-right"></i> {{ subsection.name }} {% if subsection.js %}<span class="fl-r label _foreground fz-xs">JS</span> {% endif %}</a>
                                                </li>
                                            {% endfor %}
                                        </ul>
                                    </div>
                                {% endif %}
                            </li>
                        {% endfor %}
                    </ul>
                </nav>
            {% endblock navigation %}
            <div class="pa-m mt-m mb-xl">
                <gcse:search></gcse:search>
                <gcse:searchresults linkTarget=”_parent”></gcse:searchresults>
            </div>
        </aside>
        <div class="__main">
            <header class="fc-header">
                <div class="fc-header-top mobile tablet">
                    <a data-fc-modules="side-panel" href="#menu" data-fc-clone="false" data-fc-remove="false" data-fc-max-width="500px" class="button _small _hollow mt-s ml-m mr-n"><i class="icon-bars mr-n"></i></a>
                    <a title="Frontend Core" href="{{ relative_path ~ urlbase }}index.html" class="fc-header-logo"><img src="{{ relative_path ~ url.static }}img/logo-frontendcore.png" class="va-m ml-s" alt="FRONTEND CORE" /></a>
                </div>
                <div class="row">
                    <div class="col">
                        <div class="box _empty pv-n mb-n">
                            <h1 class="h2">{{ current_children }}</h1>
                            {% if current_subtitle %}
                                <p>{{ current_subtitle }}</p>
                            {% endif %}
                        </div>
                    </div>
                </div>
            </header>
            <div class="box _empty">
                {% block main %}{% endblock main %}
            </div>
        </div>
    </div>


{% endblock fullmain %}
<script type="text/javascript" async="async">
    var oGlobalSettings = {
        sPathJs : '/{{ relative_path ~ url.static }}js/',
        sPathJsCore: '/{{ relative_path ~ url.static }}js/',
        sPathJsModules : '{{ relative_path ~ url.static }}js/modules/',
        sPathJsLibs : '{{ relative_path ~ url.static }}js/libs/',
        sPathCss: '{{ relative_path ~ url.static }}css/',
        bResponsiveImages: true,
        sHash : '{{ version|replace({'.':''}) }}',
        oCss: {
            '{{ relative_path ~ url.static }}css/main.css?v={{ version|replace({'.':''}) }}' : 'all',
            '{{ relative_path ~ url.static }}css/icons.css?v={{ version|replace({'.':''}) }}' : 'all',
            'https://fonts.googleapis.com/css?family=Alegreya+Sans' : 'all',
            'https://fonts.googleapis.com/css?family=Source+Sans+Pro' : 'all',
            '{{ relative_path ~ url.static }}css/screen-xs.css?v={{ version|replace({'.':''}) }}' : 'screen and (max-width:249px)',
            '{{ relative_path ~ url.static }}css/screen-s.css?v={{ version|replace({'.':''}) }}' : 'screen and (min-width:249px) and (max-width:499px)',
            '{{ relative_path ~ url.static }}css/screen-m.css?v={{ version|replace({'.':''}) }}' : 'screen and (min-width:500px) and (max-width:799px)',
            '{{ relative_path ~ url.static }}css/screen-l.css?v={{ version|replace({'.':''}) }}' : 'screen and (min-width:799px) and (max-width:979px)',
            '{{ relative_path ~ url.static }}css/screen-xl.css?v={{ version|replace({'.':''}) }}' : 'screen and (min-width:980px) and (max-width:1279px)',
            '{{ relative_path ~ url.static }}css/screen-xxl.css?v={{ version|replace({'.':''}) }}' : 'screen and (min-width:1280px) and (max-width:1439px)',
            '{{ relative_path ~ url.static }}css/screen-xxxl.css?v={{ version|replace({'.':''}) }}' : 'screen and (min-width:1440px)'
        }
    }
</script>
<script async="async" src="{{ relative_path ~ url.static }}js/frontendcore.js?v={{ version|replace({'.':''}) }}"></script>
<script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-60626121-1', 'auto');
    ga('send', 'pageview');

</script>
