{
  "scss" : {
    "cwd": "scss",
    "dest": "build/static/css",
    "browsers" : ["last 1 version"]
  },
  "js" : {
    "cwd": "js",
    "dest": "build/static/js",
    "uglify": true,
    "jshint" : true
  },
  "icons" : {
    "cwd" : "components/icons/font-awesome",
    "dest": "build/static/css",
    "destHtml" : "components/icons/html"
  },
  "bower" : {
    "cwd": "components"
  }
}